bin/project.out: src/project.c bin/mathgps.o bin/log.o bin/pfcs_util.o\
bin/gpgll.o bin/string_util.o bin/time_util.o bin/pipe_channel.o\
bin/socket_channel.o bin/file_channel.o bin/pfcs.o bin/transducers.o
	cc src/project.c bin/mathgps.o bin/log.o bin/pfcs_util.o bin/gpgll.o bin/string_util.o bin/time_util.o bin/pipe_channel.o bin/socket_channel.o bin/file_channel.o bin/pfcs.o bin/transducers.o -o bin/project.out

